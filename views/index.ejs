<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <ul>
      <% messages.forEach(function(message) { %>
        <li><strong> Message: </strong><%= message.text %>, <strong>User: </strong> <%= message.user%>, 
          <strong>Added date: </strong> <%= message.added %></li>
      <% }); %>
    </ul>
  </body>
</html>
